{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/main",
    "test": "jest",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js"
  },
  "dependencies": {
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@nestjs/passport": "^1.1.0",
    "@nestjs/typeorm": "^5.0.0",
    "bcrypt": "^3.0.0",
    "dotenv": "^6.0.0",
    "joi": "^13.4.0",
    "jsonwebtoken": "^8.3.0",
    "mysql": "^2.15.0",
    "node-schedule": "^1.3.0",
    "nodemailer": "^4.6.7",
    "nodemailer-mailgun-transport": "^1.4.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "typeorm": "^0.1.16",
    "typescript": "^2.8.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/faker": "^4.1.2",
    "@types/jest": "^19.2.3",
    "@types/node": "^7.0.41",
    "@types/supertest": "^2.0.4",
    "faker": "^4.1.0",
    "jest": "^20.0.3",
    "supertest": "^3.1.0",
    "ts-jest": "^20.0.3",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(e2e/.*|\\.(e2e-spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
